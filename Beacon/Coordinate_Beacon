
//비콘 인식 경로를 시각화할 때 사용하는 좌표
Beacon-1-> Beacon-2 갈 떄   85,75		415,325
Beacon-2-> Beacon-3 갈 때   470,325	670,80
Beacon-3-> Beacon-1 갈 때   85,55		670,55



beacon1 = [85, 55]  #1번 비콘 아래
beacon1_2 = [85, 75] #1번 비콘 위
beacon2 = [415, 325] #2번 비콘 왼쪽
beacon2_2 = [470, 325] #2번 비콘 오른쪽
beacon3 = [670, 85] #3번 비콘 위쪽
beacon3_2 = [670, 55] #3번 비콘 아래쪽
